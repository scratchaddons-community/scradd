{
	"name": "scradd",
	"version": "2.4.0",
	"private": true,
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "tsc --project tsconfig.json --watch",
		"format": "prettier --write \"**\" \"!dist/**\" --ignore-unknown",
		"lint": "cd node_modules/@redguy12/eslint-config/ && npx eslint ../../../commands --resolve-plugins-relative-to . --config ../../../.eslintrc.cjs",
		"serve": "nodemon . --watch dist",
		"start": "node ."
	},
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"@discordjs/opus": "0.9.0",
		"@discordjs/voice": "0.14.0",
		"@napi-rs/canvas": "0.1.30",
		"async-exit-hook": "2.0.1",
		"difflib": "0.2.4",
		"discord.js": "14.7.1",
		"dotenv": "16.0.3",
		"ffmpeg-static": "5.1.0",
		"fuse.js": "6.6.2",
		"json-diff": "0.9.1",
		"libsodium-wrappers": "0.7.10",
		"papaparse": "5.3.2",
		"serialize-error": "11.0.0"
	},
	"devDependencies": {
		"@railway/cli": "2.0.13",
		"@redguy12/eslint-config": "file:../../configs/eslint-config",
		"@redguy12/prettier-config": "2.0.2",
		"@types/async-exit-hook": "<=2.0.1",
		"@types/chrome": "latest",
		"@types/eslint": "8.4.10",
		"@types/json-diff": "<=0.9.1",
		"@types/node": "<=18.12.1",
		"@types/papaparse": "<=5.3.2",
		"eslint": "8.30.0",
		"nodemon": "2.0.20",
		"prettier": "2.8.0",
		"typescript": "4.9.4"
	},
	"engines": {
		"node": "18.12.1",
		"npm": "8.13.2"
	}
}
