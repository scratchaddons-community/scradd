		"date-fns": "2.30.0",
		"difflib": "npm:@ewoudenberg/difflib@0.1.0",
		"discord.js": "dev",
		"dotenv": "16.3.1",
		"json-diff": "1.0.6",
		"match-sorter": "6.3.1",
		"papaparse": "5.4.1",
