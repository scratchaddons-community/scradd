		"posthtml-parser": "0.12.1",
		"strife.js": "4.2.0"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "3.4.3",
		"@total-typescript/ts-reset": "0.6.1",
		"@types/node": "<=22.10",
		"eslint": "9.26.0",
		"eslint-config-cobaltt7": "1.2.0",
