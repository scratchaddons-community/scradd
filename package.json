{
	"name": "scradd",
	"version": "2.0.4",
	"private": true,
	"description": "Custom Discord bot for the Scratch Addons server.",
	"homepage": "https://discord.gg/FPv957V6SD",
	"repository": {
		"type": "git",
		"url": "https://github.com/scratchaddons-community/scradd"
	},
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "tsc --project tsconfig.json --watch",
		"format": "prettier --write \"**\" \"!dist/**\" --ignore-unknown",
		"prepare": "cp pre-commit .git/hooks/ && chmod ug+x .git/hooks/",
		"serve": "nodemon . --watch dist",
		"start": "node ."
	},
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"async-exit-hook": "2.0.1",
		"difflib": "0.2.4",
		"discord.js": "14.5.0",
		"dotenv": "16.0.2",
		"fuse.js": "6.6.2",
		"json-diff": "0.9.0",
		"node-fetch": "3.2.10",
		"papaparse": "5.3.2",
		"serialize-error": "11.0.0"
	},
	"devDependencies": {
		"@railway/cli": "2.0.12",
		"@redguy12/prettier-config": "2.0.0",
		"@types/async-exit-hook": "<=2.0.1",
		"@types/chrome": "0.0.197",
		"@types/json-diff": "<=0.9.0",
		"@types/node": "<=16.17.0",
		"@types/papaparse": "<=5.3.2",
		"nodemon": "2.0.20",
		"prettier": "2.7.1",
		"typescript": "4.8.3"
	},
	"engines": {
		"node": "16.17.0",
		"npm": "8.13.2"
	}
}
